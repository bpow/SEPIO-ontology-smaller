    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/SEPIO_0000197">
        <obo:IAO_0000115>A relationship that holds between an assertion and a term that summarizes its asserted conclusion.</obo:IAO_0000115>
        <terms:source>ClinGen (http://datamodel.clinicalgenome.org/interpretation/master/tech/variant_interpretation.html)</terms:source>
        <rdfs:label xml:lang="en">asserted_outcome</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/SEPIO_0000232 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/SEPIO_0000232">
        <rdfs:label xml:lang="en">has_quantifier</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/SEPIO_0000255 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/SEPIO_0000255">
        <obo:IAO_0000115>A relation that holds between a statement and a method that was used in generating the statement or any data that supports it.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">has_supporting_method</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/SEPIO_0000257 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/SEPIO_0000257">
