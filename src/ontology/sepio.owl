        <obo:IAO_0000118>assertion_trust_rating</obo:IAO_0000118>
        <rdfs:comment>Different data sources often create their own scales or value sets to capture this concept.  e.g. CIViC &apos;Trust Ratings&apos; are based on a 5 star system.</rdfs:comment>
        <rdfs:label xml:lang="en">assertion_confidence_level</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/SEPIO_0000169 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/SEPIO_0000169">
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/SEPIO_0000158"/>
